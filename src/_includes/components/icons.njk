<!-- SVG Icon Helper Functions -->
<script>
  function createSvgIcon(iconId, className = '', size = 24) {
    const svg = document.createElementNS('http://www.w3.org/2000/svg', 'svg');
    svg.setAttribute('width', size);
    svg.setAttribute('height', size);
    svg.setAttribute('viewBox', '0 0 24 24');
    svg.setAttribute('fill', 'currentColor');
    svg.setAttribute('aria-hidden', 'true');
    
    if (className) {
      svg.setAttribute('class', className);
    }
    
    const use = document.createElementNS('http://www.w3.org/2000/svg', 'use');
    use.setAttributeNS('http://www.w3.org/1999/xlink', 'href', `#${iconId}`);
    svg.appendChild(use);
    
    return svg;
  }
  
  function replaceFontAwesomeIcons() {
    // Replace Font Awesome icons with inline SVGs
    const iconMapping = {
      'fa-moon': 'icon-moon',
      'fa-sun': 'icon-sun', 
      'fa-envelope': 'icon-envelope',
      'fa-phone': 'icon-phone',
      'fa-linkedin': 'icon-linkedin',
      'fa-github': 'icon-github',
      'fa-arrow-up': 'icon-arrow-up',
      'fa-arrow-right': 'icon-arrow-right',
      'fa-external-link-alt': 'icon-external-link-alt',
      'fa-calendar': 'icon-calendar',
      'fa-clock': 'icon-clock',
      'fa-map-marker-alt': 'icon-map-marker-alt',
      'fa-graduation-cap': 'icon-graduation-cap',
      'fa-briefcase': 'icon-briefcase',
      'fa-heart': 'icon-heart',
      'fa-chart-line': 'icon-chart-line',
      'fa-chart-bar': 'icon-chart-bar',
      'fa-database': 'icon-database',
      'fa-users-cog': 'icon-users-cog',
      'fa-cogs': 'icon-cogs',
      'fa-code': 'icon-code',
      'fa-chess': 'icon-chess',
      'fa-users': 'icon-users',
      'fa-handshake': 'icon-handshake',
      'fa-tasks': 'icon-tasks',
      'fa-comments': 'icon-comments',
      'fa-bullseye': 'icon-bullseye',
      'fa-certificate': 'icon-certificate',
      'fa-laptop-code': 'icon-laptop-code',
      'fa-trophy': 'icon-trophy',
      'fa-paper-plane': 'icon-paper-plane',
      'fa-info-circle': 'icon-info-circle',
      'fa-spinner': 'icon-spinner'
    };
    
    // Replace all font-awesome icons
    Object.keys(iconMapping).forEach(faClass => {
      const elements = document.querySelectorAll(`.${faClass}`);
      elements.forEach(element => {
        const newSvg = createSvgIcon(iconMapping[faClass], element.className);
        element.parentNode.replaceChild(newSvg, element);
      });
    });
  }
  
  // Auto-replace when DOM is loaded
  if (document.readyState === 'loading') {
    document.addEventListener('DOMContentLoaded', replaceFontAwesomeIcons);
  } else {
    replaceFontAwesomeIcons();
  }
</script>